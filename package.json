{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev  -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "cap:sync": "npx cap sync",
    "build-mobile": "npm run build && npm run cap:sync",
    "postinstall": "patch-package && node -e \"const fs = require('fs'); const path = require('path'); try { if (fs.existsSync('node_modules/@ffmpeg/core/dist')) { fs.mkdirSync('public/ffmpeg', { recursive: true }); fs.cpSync('node_modules/@ffmpeg/core/dist', 'public/ffmpeg', { recursive: true }); console.log('Copied ffmpeg assets.'); } else { console.log('ffmpeg-core not found, skipping copy'); } } catch(e) { console.error('Error copying ffmpeg files:', e.message); }\" && node -e \"const fs = require('fs'); const path = require('path'); try { fs.mkdirSync('public/assets', { recursive: true }); fs.copyFileSync('node_modules/jeep-sqlite/dist/jeep-sqlite/sql-wasm.wasm', 'public/assets/sql-wasm.wasm'); console.log('Copied sql-wasm.wasm.'); } catch(e) { console.error('Error copying jeep-sqlite wasm:', e.message); }\""
  },
